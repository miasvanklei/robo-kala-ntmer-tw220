<DSIInitSequence>
    FF 32 <!-- wait for 50 ms -->
    FD 03 <!-- 1 command is send to controller 0, 2 command is send to controller 1, 3 to both? -->
    15 FE D1
    15 D3 00
    15 D0 00
    15 D2 00
    15 D4 00
    15 B4 01
    15 FE A1
    15 B1 04
    15 CD 6B
    15 CE BB
    15 FE 40
    15 B3 08
    15 FE 26
    15 32 07
    15 FE FD
    15 80 04
    15 83 00
    15 FE D0
    15 7E 80
    15 FE D2
    15 23 56
    15 FE A1
    15 B3 3F
    15 E3 03
    15 C3 83
    15 C4 FF
    15 C5 5F
    15 FE FD
    15 EE 00
    15 EF 00
    15 FE A0
    15 40 3C <!-- 60 hz -->
    15 FE 00
    15 53 28 <!-- write ctrl display -->
    15 C2 08
    15 35 00 <!-- turn on tearing -->
    39 51 00 00 <!-- set display brightness -->
    05 11 00 <!-- exit sleep mode -->
    FF 70 <!-- wait for 112 ms -->
    05 29 00 <!-- display on -->
    FF 30 <!-- wait for 48 ms -->
</DSIInitSequence>
<DSITermSequence>
    FD 03
    15 FE 00
    05 28
    FF 28
    05 10
    FF 28
</DSITermSequence>
